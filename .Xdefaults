! .f - X defaults

Xft.antialias:			0
Xft.autohint:			0
Xft.hinting:			1
Xft.hintstyle:			hintfull
Xft.lcdfilter:			lcddefault
Xft.rgba:			rgb

! urxvt-specific config
urxvt.boldMode:			false
urxvt.borderless:		false
urxvt.borderColor:		#151515
urxvt.cursorBlink:		false
urxvt.internalBorder:		0
urxvt.geometry:			80x25
urxvt.saveLines:		0
urxvt.bell-command:		/dev/null
urxvt.urgentOnBell:		False
!urxvt.perl-ext-common:		default,matcher
!urxvt.perl-ext:		default,url-select
!urxvt.keysym-M-u:		perl:url-select:select next
!urxvt.url-select.underline:	true
!urxvt.url-select.launcher:	$BROWSER
urxvt.url-launcher:		$BROWSER
urxvt.matcher.button:		1
urxvt.colorUL:			#3388cc
urxvt.termName:			rxvt-unicode-256color
urxvt.xftAntialias:		false
urxvt.cursorColor:		#aaa
urxvt.pointerColor:		#aaa
! disable keycap insert 
urxvt.iso14755:			false
urxvt.iso14755_52:		false

! global settings
*scrollBar:			none
*font:				-jmk-neep-medium-r-*--11-100-75-75-c-60-*-1
*italicFont:			-jmk-neep-medium-r-*--11-100-75-75-c-60-*-1
*boldFont:			-jmk-neep-bold-r-*--11-100-75-75-c-60-*-1
*boldItalicfont:		-jmk-neep-bold-r-*--11-100-75-75-c-60-*-1

! colours
*foreground:			#839496
*background:			#121315
*color0:			#3f3f4c
*color1:			#ac6a76
*color2:			#7b8c58
*color3:			#bc945a
*color4:			#58698c
*color5:			#7b5e8d
*color6:			#82a1b2
*color7:			#cccaca
*color8:			#525263
*color9:			#c47987
*color10:			#90a366
*color11:			#d4a765
*color12:			#7086b2
*color13:			#9572ab
*color14:			#95b9cc
*color15:			#edeff2

# vim: set ft=xdefaults :
